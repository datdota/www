{
  "name": "datdota-www",
  "description": "datdota Web Front-End",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ci": "yarn test --silent",
    "lint": "yarn test:lint",
    "format": "yarn test:lint --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "prepare": "is-ci || husky install",
    "test": "run-s test:*",
    "test:lint": "eslint \"./src/**/*.{j,t}s?(x)\"",
    "test:unit": "jest -c ./jest.config.js",
    "test:typecheck": "tsc --noEmit",
    "typecheck": "yarn test:typecheck"
  },
  "dependencies": {
    "@renderlesskit/react": "^0.7.2",
    "next": "^12.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.28.0",
    "reakit": "^1.3.10",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.0",
    "@saeris/eslint-config": "^2.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.15",
    "@types/stylelint": "^13.13.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-plugin-styled-components": "^1.13.3",
    "commitlint": "^13.2.1",
    "eslint": "^8.0.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "is-ci": "^3.0.1",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.1",
    "typescript": "^4.4.4"
  }
}
